# First set some options:
# For delaunay triangulations:
HAVE_LIB_CGAL = 0
# For more output:
VERBOSE_LIB   = 1
# For stuff that needs a LAMMPS lib:
HAVE_LIB_LAMMPS = 1
# For armadillo:
HAVE_LIB_ARMADILLO = 1
# For ICP:
HAVE_LIB_ICP = 1
# For GZIP
HAVE_BOOST_GZIP = 1

include Makefile.common

